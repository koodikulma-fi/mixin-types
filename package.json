{
  "name": "mixin-types",
  "version": "1.0.0",
  "description": "A tiny library providing typing tools for mixin classes and 2 simple JS funcs mixins (mixins and mixinsWith).",
  "keywords": [
    "mixin-types",
    "mixin",
    "types",
    "typing",
    "class",
    "mixins",
    "classes",
    "simple",
    "mixer",
    "javascript",
    "typescript",
    "js",
    "ts",
    "tools",
    "library"
  ],
  "homepage": "https://github.com/koodikulma-fi/mixin-types",
  "author": "Mikael Saur√©n",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koodikulma-fi/mixin-types.git"
  },
  "module": "./mixin-types.module.js",
  "main": "./mixin-types.js",
  "types": "./mixin-types.d.ts",
  "exports": {
    ".": {
      "import": "./mixin-types.module.js",
      "require": "./mixin-types.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "build": "tsc && rollup -c rollup.config.js"
  },
  "devDependencies": {
    "rollup": "^2.77.2",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "4.9"
  }
}
